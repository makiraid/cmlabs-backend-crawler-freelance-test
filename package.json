{
  "name": "cmlabs-backend-crawler-freelance-test",
  "version": "1.0.0",
  "description": "CMLabs Backend Crawler - Freelance Test",
  "main": "app.js",
  "standard": {
    "ignore": [
      "/node_modules/*"
    ],
    "env": [
      "jest"
    ]
  },
  "overrides": {
    "graceful-fs": "^4.2.11"
  },
  "scripts": {
    "test:unit": "jest --config=./tests/jest.unit.config.js --detectOpenHandles",
    "test:unit-ci": "NODE_ENV=test jest --config=./tests/jest.unit.config.js --detectOpenHandles --coverage --ci --reporters=default --reporters=jest-junit",
    "standard": "standard --fix --verbose | snazzy",
    "reinstall": "rm -rf node_modules && rm -rf package-lock.json && npm i",
    "precommit": "lint-staged",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^7.98.0",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "pidusage": "^3.0.2",
    "request-ip": "^3.3.0",
    "request-promise": "^4.2.6",
    "response-time": "^2.3.2",
    "sprintf-js": "^1.1.3",
    "uuid": "^9.0.1",
    "website-scraper": "^5.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0"
  }
}
